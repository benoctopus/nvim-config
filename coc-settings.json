{
  "coc.preferences.formatOnSave": true,
  "emmet.includeLanguages": {
    "typescriptreact": "javascriptreact"
  },
  "python.formatting.provider": "black",
  "emmet.priority": 1,
  "diagnostic.virtualText": true,
  "html.autoCreateQuotes": false,
  "diagnostic.virtualTextAlign": "after",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "colors.enable": true,
  "pullDiagnostic.onSave": true,
  "pullDiagnostic.onChange": false,
  "snippets.ultisnips.pythonPrompt": false,
  "sql.database": "postgresql",
  "cSpell.userWords": ["Skus", "calendly", "livemode", "reduxjs", "yardzen"],
  "snippets.textmateSnippetsRoots": ["~/snippets/"],
  "tailwindCSS.trace.server": "verbose",
  "html.autoClosingTags": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "svelte.enable-ts-plugin": true
}
